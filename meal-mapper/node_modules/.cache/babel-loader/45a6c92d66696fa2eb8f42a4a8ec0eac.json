A;;AAEA,UAAA,KAAA,MAAA,EAAA;AACA,QAAA,gBAAA,CAAA,OAAA,CAAA,UAAA,IAAA,EAAA,KAAA,EAAA;AACA,cAAA,OAAA,GAAA,KAAA,CAAA,EAAA,qBAAA,YAAA,CAAA,KAAA,CAAA,CAAA,GAAA,EAAA;;AACA,kBAAA,KAAA,CAAA,QAAA,CAAA,IAAA,EAAA,EAAA,CAAA,MAAA;AACA,iBAAA,CAAA;AACA;AACA;AACA;;AACA,iBAAA,CAAA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AACA;AACA,cAAA,MAAA,CAAA,IAAA,CAAA;AAAA,gBAAA,IAAA,EAAA,OAAA;AAAA,gBAAA,GAAA,EAAA,KAAA,CAAA,QAAA,CAAA,IAAA,EAAA,EAAA,CAAA,OAAA,CAAA,GAAA,EAAA,OAAA;AAAA,eAAA;AACA,cAAA,GAAA;AACA;AAhBA;AAkBA,SApBA;AAqBA,OAtBA,MAsBA;AACA,QAAA,UAAA,CAAA,OAAA,CAAA,UAAA,IAAA,EAAA,KAAA,EAAA;AACA,cAAA,OAAA,GAAA,KAAA,CAAA,EAAA,qBAAA,YAAA,CAAA,KAAA,CAAA,CAAA,GAAA,EAAA;;AACA,kBAAA,KAAA,CAAA,QAAA,CAAA,IAAA,EAAA,EAAA,CAAA,MAAA;AACA,iBAAA,CAAA;AACA;AACA;AACA;;AACA,iBAAA,CAAA;AACA,kBAAA,KAAA,CAAA,QAAA,CAAA,IAAA,EAAA,EAAA,IAAA,CAAA,EAAA;AACA,gBAAA,MAAA,CAAA,IAAA,CAAA;AAAA,kBAAA,IAAA,EAAA,OAAA;AAAA,kBAAA,GAAA,EAAA,KAAA,CAAA,EAAA,CAAA,cAAA,CAAA;AAAA,kBAAA,KAAA,EAAA;AAAA,iBAAA;AACA,eAFA,MAEA,CACA;AACA;AACA;;AACA,cAAA,GAAA;AACA;;AACA;AACA,cAAA,MAAA,CAAA,IAAA,CAAA;AAAA,gBAAA,IAAA,EAAA,OAAA;AAAA,gBAAA,GAAA,EAAA,KAAA,CAAA,QAAA,CAAA,IAAA,EAAA,EAAA,CAAA,OAAA,CAAA,GAAA,EAAA,OAAA;AAAA,eAAA;AACA,cAAA,GAAA;AACA;AAjBA;AAmBA,SArBA;AAsBA;;AAEA,UAAA,GAAA,IAAA,CAAA,EAAA;AACA,eAAA,MAAA,CAAA,IAAA,CAAA;AAAA,UAAA,IAAA,EAAA,KAAA,EAAA,CAAA,eAAA,CAAA;AAAA,UAAA,GAAA,EAAA,KAAA,EAAA,CAAA,cAAA;AAAA,SAAA,CAAA;AACA;;AAEA,aAAA,MAAA;AACA;AAzDA;AAbA,CAAA","sourcesContent":["<template>\n  <div class=\"openhours\" v-if=\"days.length > 0\">\n    <div class=\"oh-title\">\n      <b>{{ title }}</b>\n    </div>\n    <table>\n      <tr v-for=\"(item, index) in days\" :key=\"index\">\n        <!-- <i class=\"fas\" :class=\"icon\" /> -->\n        <td class=\"oh-name\">{{ item.name }}</td>\n        <td :class=\"item.class\"><span v-html=\"item.val\"></span></td>\n      </tr>\n    </table>\n  </div>\n</template>\n\n<script>\nimport { dayFilters, seniorDayFilters, weekdayHours } from '../constants'\nexport default {\n  name: 'OpeningHours',\n  data() {\n    return {}\n  },\n  props: {\n    title: { type: String },\n    infotype: { type: String },\n    icon: { type: String },\n    senior: { type: Boolean },\n    business: { type: Object },\n    day: { type: String }\n  },\n  computed: {\n    days() {\n      var myDays = []\n      var cnt = 0\n\n      if (this.senior) {\n        seniorDayFilters.forEach((attr, index) => {\n          var dayName = this.$t(`dayofweek.${weekdayHours[index].day}`)\n          switch (this.business[attr].$t.length) {\n            case 0:\n              // myDays.push({ name: dayName, val: this.$t('label.normalhours') })\n              // cnt++\n              break\n            case 1:\n              // if (this.business[attr].$t == '0') {\n              //   myDays.push({ name: dayName, val: this.$t('label.closed') })\n              // } else {\n              //   // myDays.push({ name: dayName, val: this.$t('label.normalhours') })\n              //   // cnt++\n              // }\n              break\n            default:\n              myDays.push({ name: dayName, val: this.business[attr].$t.replace(',', '<br/>') })\n              cnt++\n              break\n          }\n        })\n      } else {\n        dayFilters.forEach((attr, index) => {\n          var dayName = this.$t(`dayofweek.${weekdayHours[index].day}`)\n          switch (this.business[attr].$t.length) {\n            case 0:\n              // myDays.push({ name: dayName, val: this.$t('label.normalhours') })\n              // cnt++\n              break\n            case 1:\n              if (this.business[attr].$t == 0) {\n                myDays.push({ name: dayName, val: this.$t('label.closed'), class: 'closed' })\n              } else {\n                // myDays.push({ name: dayName, val: this.$t('label.normalhours') })\n                // cnt++\n              }\n              cnt++\n              break\n            default:\n              myDays.push({ name: dayName, val: this.business[attr].$t.replace(',', '<br/>') })\n              cnt++\n              break\n          }\n        })\n      }\n\n      if (cnt == 0) {\n        return myDays.push({ name: this.$t('label.allweek'), val: this.$t('label.closed') })\n      }\n\n      return myDays\n    }\n  }\n}\n</script>\n\n<style lang=\"scss\">\n.openhours {\n  margin-bottom: 10px;\n}\n.oh-name {\n  padding-right: 20px;\n  vertical-align: top;\n}\n.closed {\n  color: theme-color('danger');\n  @media (prefers-color-scheme: dark) {\n    color: theme-color-level('danger', 2);\n  }\n}\n</style>\n"],"sourceRoot":"src/components"}]}